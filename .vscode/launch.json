{
  "version": "0.1.0",
  "configurations": [
    {
      "name": "(MSVC) Search for best seeds",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${command:cmake.launchTargetPath}",
      "args": ["seed", "0"],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "externalConsole": true
    },
    {
      "name": "(MSVC) Search for quad temple",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${command:cmake.launchTargetPath}",
      "args": ["quad", "0"],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "externalConsole": true
    },
    {
      "name": "(MSVC) Search for best location in a seed",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${command:cmake.launchTargetPath}",
      "args": ["loc", "1306145184061456995"],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "externalConsole": true
    }
  ]
}
